# Stacks NFT Minter Environment Configuration
# Copy this file to .env and fill in your values
# NOTE: Vite requires VITE_ prefix for client-side variables

# ===========================================
# NETWORK CONFIGURATION
# ===========================================
# Options: mainnet, testnet, devnet
VITE_STACKS_NETWORK=mainnet

# Hiro API endpoint
VITE_STACKS_API_URL=https://api.mainnet.hiro.so

# ===========================================
# CONTRACT CONFIGURATION
# ===========================================
# Your deployed NFT contract address (principal)
VITE_CONTRACT_ADDRESS=
# Contract name
VITE_CONTRACT_NAME=stacks-nft-v2

# ===========================================
# APP CONFIGURATION
# ===========================================
# Application name shown in wallet connection
VITE_APP_NAME=Stacks NFT Minter

# App icon URL for wallet display (optional)
VITE_APP_ICON_URL=

# ===========================================
# DEVELOPMENT CONFIGURATION
# ===========================================
# Enable debug logging (true/false)
VITE_DEBUG=false

# App version
VITE_VERSION=1.0.0

# ===========================================
# DEPLOYMENT SCRIPTS (server-side only)
# ===========================================
# WARNING: Never commit actual keys!
# STACKS_PRIVATE_KEY=your-private-key-here
# STACKS_TEST_PRIVATE_KEY=your-test-private-key-here